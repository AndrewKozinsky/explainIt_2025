	"scripts": {
		"format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
		"start": "nest start",
		"start:dev": "nest start --watch",
		"start:debug": "nest start --debug --watch",
		"start:prod": "node dist/src/main",
		"build": "nest build",
		"generatePrismaFile": "ts-node src/db/prismaGenerator/generatePrismaSchemaFile.ts",
		"generate": "prisma generate",
		"migrate:dev": "prisma migrate dev",
		"migrate:deploy": "prisma migrate deploy",
		"migrate:generate-types": "prisma generate client",
		"test": "jest --config src/jest.json --runInBand",
		"test:watch": "jest --watch",
		"test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
		"test:e2e": "jest --config e2e/jest-e2e.ts --runInBand"
	},
	"dependencies": {
		"@apollo/server": "^4.12.0",
		"@aws-sdk/client-s3": "^3.958.0",
