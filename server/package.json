		"@types/express-session": "^1.18.2",
		"@types/ioredis-mock": "^8.2.6",
		"@types/jest": "^29.5.14",
		"@types/node": "^24.10.2",
		"@types/supertest": "^6.0.3",
		"dotenv-cli": "^11.0.0",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-prettier": "^5.2.2",
		"globals": "^16.0.0",
		"ioredis-mock": "^8.9.0",
