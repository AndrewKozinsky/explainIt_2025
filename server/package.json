		"date-fns": "^4.1.0",
		"dotenv": "^17.2.3",
		"express-session": "^1.18.1",
		"express-useragent": "^1.0.15",
		"graphql": "^16.10.0",
		"ioredis": "^5.8.2",
		"openai": "^6.10.0",
		"redis": "^5.10.0",
		"reflect-metadata": "^0.2.2",
		"rxjs": "^7.8.1",
		"sendpulse-api": "^1.1.7",
		"ts-morph": "^25.0.1",
		"uuid": "^11.1.0",
		"zod": "^4.1.13"
	},
	"devDependencies": {
		"@eslint/eslintrc": "^3.2.0",
		"@eslint/js": "^9.18.0",
