		"@nestjs/config": "^4.0.2",
		"@nestjs/core": "^11.1.9",
		"@nestjs/cqrs": "^11.0.3",
		"@nestjs/graphql": "^13.1.0",
		"@nestjs/platform-express": "^11.1.9",
		"@prisma/adapter-pg": "^7.1.0",
		"@prisma/client": "^7.1.0",
		"axios": "^1.13.2",
		"class-transformer": "^0.5.1",
